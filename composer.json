{
    "name": "juanparati/sendinblue",
    "description": "A Sendinblue v3 interface provider for Laravel",
    "type": "library",
    "keywords": [
        "Sendinblue",
        "laravel",
        "mail",
        "email"
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/mail": "^9",
        "sendinblue/api-v3-sdk": "^8.2.1",
        "symfony/sendinblue-mailer": "6.0.8",
        "symfony/http-client": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpoption/phpoption": "^1.7@dev",
        "vlucas/phpdotenv": "^5.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\Sendinblue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\Sendinblue\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juanparati\\Sendinblue\\Providers\\SendinblueProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
