{
    "name": "juanparati/sendinblue",
    "description": "A Sendinblue v3 interface provider for Laravel 5.5+/6.x/7.x",
    "type": "library",
    "keywords": [
        "Sendinblue",
        "laravel",
        "mail",
        "email"
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/mail": "^7",
        "sendinblue/api-v3-sdk": "~6.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\Sendinblue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\Sendinblue\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juanparati\\Sendinblue\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
